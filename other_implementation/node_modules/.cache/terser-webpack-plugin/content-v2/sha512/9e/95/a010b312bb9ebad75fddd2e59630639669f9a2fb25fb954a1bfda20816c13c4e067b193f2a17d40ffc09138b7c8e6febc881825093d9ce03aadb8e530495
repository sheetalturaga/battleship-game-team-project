*(end-start+1)+start);};var PlayerBoardAction_incrementCounter=function incrementCounter(ship){PlayerBoardAction_COUNTER[ship]+=1;};var PlayerBoardAction_checkIfAllShipsHit=function checkIfAllShipsHit(ship){if(PlayerBoardAction_COUNTER[ship]===PlayerBoardAction_SHIPS_OBJ[ship]){return true;}};var PlayerBoardAction_destroyShips=function destroyShips(state,ship){for(var i=0;i<PlayerBoardAction_SHIPS_OBJ[ship];i++){var coordX=PlayerBoardAction_MAP_OF_SHIP_COORDS[ship][i][0];var coordY=PlayerBoardAction_MAP_OF_SHIP_COORDS[ship][i][1];state[coordX][coordY]='S';}};var addUsedSquare=function addUsedSquare(xCoord,yCoord){TRACKING_USED_SQUARE.push([xCoord,yCoord]);};var checkIfValidCoordinate=function checkIfValidCoordinate(state,xCoord,yCoord){for(var i=0;i<TRACKING_USED_SQUARE.length;i++){if(TRACKING_USED_SQUARE[i][0]===xCoord&&TRACKING_USED_SQUARE[i][1]===yCoord){return false;}}return true;};\n// CONCATENATED MODULE: ./src/reducers/PlayerBoardReducer.js\nfunction PlayerBoardReducer(state,action){if(state===undefined){return PlayerBoardAction_generateEmptyBoard();}if(action.type===\"CREATE_GAME_BOARD\"){alert(\"created a game board!\");}if(action.type==='onClick'){var value=state[action.x][action.y];var boardType=action.boardType;if(boardType==='computer'){var xCoord=PlayerBoardAction_generateRandomNumber(0,state.length-1);var yCoord=PlayerBoardAction_generateRandomNumber(0,state.length-1);while(!checkIfValidCoordinate(state,xCoord,yCoord)){xCoord=PlayerBoardAction_generateRandomNumber(0,state.length-1);yCoord=PlayerBoardAction_generateRandomNumber(0,state.length-1);continue;}if(state[xCoord][yCoord]==='sct'){PlayerBoardAction_incrementCounter('scout');PlayerBoardAction_checkIfAllShipsHit('scout')?PlayerBoardAction_destroyShips(state,'scout'):state[xCoord][yCoord]='O';if(state[xCoord][yCoord]='O'){addUsedSquare(xCoord,yCoord);}}else if(state[xCoord][yCoord]==='sbe'){PlayerBoardAction_incrementCounter('submarine');PlayerBoardAction_checkIfAllShipsHit('submarine')?PlayerBoar